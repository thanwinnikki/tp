<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>

<GridPane styleClass="status-bar" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1">
  <columnConstraints>
    <ColumnConstraints hgrow="SOMETIMES" minWidth="10" />
  </columnConstraints>
  <Label fx:id="saveLocationStatus" alignment="CENTER_RIGHT" GridPane.columnIndex="0"/>
  <Label fx:id="applicationStateType" text="\$state" styleClass="cell_big_label" GridPane.columnIndex="1" wrapText="true" maxWidth="300"/>
</GridPane>
